{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
    "no-console": "off",
    "indent": ["error", 2],
    "no-underscore-dangle": "off",
    "linebreak-style": "off"
    }
}
